{
  "name": "db-viewer-component",
  "version": "3.6.3",
  "description": "A web component for showing db schemas",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayeressian/db-viewer-component"
  },
  "keywords": [
    "database",
    "db",
    "schema",
    "viewer"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config=./webpack.config/webpack.config.js",
    "build-prod": "webpack --mode=production --config=./webpack.config/webpack.config.js",
    "build-dev": "webpack --mode=development --watch --config=./webpack.config/webpack.config.js",
    "publish-npm": "rm -rf dist && npm run build-prod && npm publish",
    "integration-test-mocha": "mocha --timeout 15000 'integration/**/*.test.js'",
    "integration-test-server": "webpack-dev-server --mode development --port 6001",
    "integration-test": "start-server-and-test integration-test-server http://localhost:6001 integration-test-mocha",
    "test": "mocha-webpack \"test/**/*.js\" --mode development --webpack-config=./webpack.config/webpack.test.config.js",
    "test:watch": "npm run test -- --watch",
    "test:debug": "webpack-dev-server --config webpack.config/webpack.test.debug.config.js --hot --inline"
  },
  "main": "dist/main.js",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "esm": "^3.2.22",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "img-loader": "^3.0.0",
    "mocha": "^5.2.0",
    "mocha-loader": "^2.0.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "puppeteer": "^1.14.0",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "start-server-and-test": "^1.7.13",
    "url-loader": "^1.1.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "ajv": "^6.10.0"
  }
}
