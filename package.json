{
  "name": "db-viewer-component",
  "version": "2.0.0",
  "description": "A web component for showing db schemas",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayeressian/db-viewer-component"
  },
  "keywords": [
    "database",
    "db",
    "schema",
    "viewer"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build-prod": "webpack --mode=production",
    "build-dev": "webpack --mode=development --watch",
    "publish-npm": "rm -rf dist && npm run build-prod && npm publish",
    "test-mocha": "mocha --timeout 15000 'spec/**/*.spec.js'",
    "test-server": "webpack-dev-server --mode development --port 6001",
    "test": "start-server-and-test test-server http://localhost:6001 test-mocha"
  },
  "main": "dist/main.js",
  "author": "",
  "license": "GNU 3",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "chai": "^4.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "esm": "^3.0.84",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "mocha": "^5.2.0",
    "puppeteer": "^1.11.0",
    "start-server-and-test": "^1.7.11",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14"
  }
}
